<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_main"
    app:startDestination="@id/home_fragment">

    <fragment
        android:id="@+id/home_fragment"
        android:label="Location"
        android:name="com.github.app.ui.main.home.HomeFragment">
        <action
            android:id="@+id/action_home_fragment_to_redeem_code_fragment2"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@+id/home_fragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_home_fragment_to_redeem_code_fragment"
            app:destination="@id/redeem_code_fragment" />
        <action
            android:id="@+id/action_home_fragment_to_profile_fragment"
            app:destination="@id/profile_fragment" />
        <action
            android:id="@+id/action_home_fragment_to_my_physical_card_fragment"
            app:destination="@id/my_physical_card_fragment" />


    </fragment>


    <fragment
        android:id="@+id/profile_fragment"
        android:label="Location"
        android:name="com.github.app.ui.profile.ProfileFragment">
        <action
            android:id="@+id/action_show_to_first_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@+id/profile_fragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_profile_fragment_to_redeem_code_fragment"
            app:destination="@id/redeem_code_fragment" />
        <action
            android:id="@+id/action_profile_fragment_to_home_fragment"
            app:destination="@id/home_fragment" />
        <action
            android:id="@+id/action_profile_fragment_to_my_physical_card_fragment"
            app:destination="@id/my_physical_card_fragment" />

    </fragment>


    <fragment
        android:id="@+id/redeem_code_fragment"
        android:label="Location"
        android:name="com.github.app.ui.redeemcode.RedeemCodeFragment">
        <action
            android:id="@+id/action_show_to_first_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@+id/redeem_code_fragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_redeem_code_fragment_to_home_fragment"
            app:destination="@id/home_fragment" />
        <action
            android:id="@+id/action_redeem_code_fragment_to_profile_fragment"
            app:destination="@id/profile_fragment" />
        <action
            android:id="@+id/action_redeem_code_fragment_to_my_physical_card_fragment"
            app:destination="@id/my_physical_card_fragment" />
    </fragment>


    <fragment
        android:id="@+id/my_physical_card_fragment"
        android:label="Location"
        android:name="com.github.app.ui.ordercard.MyPhysicalCardFragment">
        <action
            android:id="@+id/action_show_to_first_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@+id/my_physical_card_fragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_my_physical_card_fragment_to_profile_fragment"
            app:destination="@id/profile_fragment" />
        <action
            android:id="@+id/action_my_physical_card_fragment_to_redeem_code_fragment"
            app:destination="@id/redeem_code_fragment" />
        <action
            android:id="@+id/action_my_physical_card_fragment_to_home_fragment"
            app:destination="@id/home_fragment" />
    </fragment>

</navigation>